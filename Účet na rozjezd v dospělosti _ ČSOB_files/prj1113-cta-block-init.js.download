;
// Init Namespaces
window.PuiTheme = window.PuiTheme || {};

/**
 * Component - PRJ1113: CTA block
 *
 * Dependencies: cat-event-dom-ready.js, cta-block.js
 *
 * @version 1.0
 */
(function (ns) {
    "use strict";

    ns.EventDomReady.subscribe('CtaBlockHtml', {}, function (e, fragment) {
        var widget = ns.CtaBlockHtml;
        if (typeof widget !== "undefined") {
            var options = {};
            widget.initWidget(options, fragment);
        }
    });
})(window.PuiTheme);